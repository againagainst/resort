{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "resort store",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/resort",
            "args": [
                "store",
                "-f",
                "projects/basic-rest-test",
            ]
        },
        {
            "name": "resort check",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/resort",
            "args": [
                "check",
                "projects/basic-rest-test"
            ]
        },
        {
            "name": "resort create",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/resort",
            "args": [
                "create",
                "projects/basic-rest-test"
            ],
            "env": {
                "PYTHONPATH":"${workspaceFolder}"
            }
        },
        {
            "name": "resort unittest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/resort/test/test_import_system.py",
            "env": {
                "PYTHONPATH":"${workspaceFolder}"
            }
        },
        {
            "name": "resort api example",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/api-unittest/test_all.py",
            "env": {
                "PYTHONPATH":"${workspaceFolder}"
            }
        },
        {
            "name": "resort get sys info",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/resort/test/sysinfo.py",
            "env": {
                "PYTHONPATH":"${workspaceFolder}"
            }
        },
        {
            "name": "Debug Basic REST server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/basic_rest/app.py",
            "args": [
            ]
        }
    ]
}